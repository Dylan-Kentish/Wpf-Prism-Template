<Window x:Class="WpfApp.Shell.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:infrastructure="clr-namespace:WpfApp.Infrastructure;assembly=WpfApp.Infrastructure"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <ContentControl regions:RegionManager.RegionName="{x:Static infrastructure:RegionNames.Root}" />
</Window>
